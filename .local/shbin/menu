#!/usr/bin/dash

OPTS="firefox\n\
minecraft\n\
ncmpcpp\n\
obs\n\
pavucontrol"
case $(echo $OPTS | fuzzel -d) in
    firefox)
        firefox &
        ;;
    minecraft)
        prismlauncher &
        ;;
    ncmpcpp)
        foot ncmpcpp &
        ;;
    obs)
        obs &
        ;;
    pavucontrol)
        pavucontrol &
        ;;
esac
