#!/usr/bin/fish
set KEYS       "key=25,x=50,y=10,w=40,h=40"     # w (up)
set KEYS $KEYS "key=38,x=10,y=50,w=40,h=40"     # a (left)
set KEYS $KEYS "key=39,x=50,y=50,w=40,h=40"     # s (down)
set KEYS $KEYS "key=40,x=90,y=50,w=40,h=40"     # d (right)

set KEYS $KEYS "key=46,x=90,y=100,w=40,h=40"    # l (grab)
set KEYS $KEYS "key=44,x=10,y=120,w=40,h=20"    # j (demo)
set KEYS $KEYS "key=45,x=10,y=100,w=40,h=20"    # k (dash)
set KEYS $KEYS "key=58,x=50,y=120,w=40,h=20"    # m (jump)
set KEYS $KEYS "key=65,x=50,y=100,w=40,h=20"    #   (jump)

input-display key=FFFFFF,background=000000 $KEYS
